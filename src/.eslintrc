{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended"
  ],
  "plugins": ["react"],
  "rules": {
    "prettier/prettier": "warn",
    "react/prop-types": 0,
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "linebreak-style": ["error", "unix"]
  },
  "parserOptions": {
    "ECMAVersion": 2021,
    "sourceType": "module",
    "ECMAFeatures": {
    "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
    "version": "detect"
    }
  }
  }
  